<script type="text/javascript">
var item = document.getElementsByClassName("item");

var cover = document.getElementsByClassName("img-full");

var userId = sessionStorage.getItem('userId');
var songId;
$(function(){
    $.ajax({
    	url:"http://localhost:8080/ChunYinBackGround/selectNewSongs",
    	data:"",
    	type:"post",
    	dataType:"jsonp",
    	success:function(data){
    		var list = data.data.newSongs;
    		for(var j=0;j<item.length;j++){
				item[j].getElementsByClassName("img-full")[0].src = list[j].songImageUrl;
				item[j].getElementsByClassName("text-ellipsis")[0].innerText = list[j].songName;
				item[j].getElementsByClassName("text-ellipsis")[1].innerText = list[j].composerName;
				item[j].getElementsByClassName("badge")[0].innerHTML = list[j].songTime;
				item[j].getElementsByClassName("icon-control-play")[0].setAttribute("song_id", list[j].songId);
				item[j].getElementsByClassName("icon-control-play")[0].onclick = function(){
					$.ajax({
			    	url:"http://localhost:8080/ChunYinBackGround/selectSongById",
			    	data:"id="+ $(this).attr("song_id"),
			    	type:"post",
			    	dataType:"jsonp",
			    	success:function(data){
			    		parent.document.getElementById("playpro").contentWindow.play(data);
			    	},
			    	error:function(){}
				    });
				};
    		}
    		
    	},
    	error:function(){}
    });
});
</script>
