.navss li > a > i {
  margin: -10px -10px;
  line-height: 38px;
  width: 38px;
  float: left;
  margin-right: 5px;
  text-align: center;
  position: relative;
  overflow: hidden;
}
.navss li > a > i:before {
  position: relative;
  z-index: 2;
}
.navss ul.nav > li > a {
  padding: 9px 20px;
  position: relative;
  transition: background-color .2s ease-in-out 0s;
}
.no-borders .navss ul.nav > li > a {
  border-width: 0 !important;
}
.navss ul.nav > li > a > .badge {
  font-size: 11px;
  padding: 2px 5px 2px 4px;
  margin-top: 2px;
}
.navss ul.nav > li > a > .text-muted {
  margin: 0 3px;
}
.navss ul.nav > li > a.active .text {
  display: none;
}
.navss ul.nav > li > a.active .text-active {
  display: inline-block !important;
}
.navss ul.nav > li li a {
  font-weight: normal;
  text-transform: none;
}
.navss ul.nav > li.active > ul {
  display: block;
}
.navss ul.nav ul {
  display: none;
}
  .nav-xs .navss > ul > li > a {
    position: relative;
    padding: 0;
    font-size: 11px;
    text-align: center;
    height: 50px;
    overflow-y: hidden;
    border: none;
  }
  .nav-xs .navss > ul > li > a span {
    display: table-cell;
    vertical-align: middle;
    height: 50px;
    width: 70px;
    padding: 0 5px;
  }
  .nav-xs .navss > ul > li > a span.pull-right {
    display: none !important;
  }
  .nav-xs .navss > ul > li > a i {
    width: auto;
    float: none;
    display: block;
    font-size: 16px;
    margin: 0;
    line-height: 50px;
    border: none !important;
    -webkit-transition: margin-top 0.2s;
    transition: margin-top 0.2s;
  }
  .nav-xs .navss > ul > li > a i b {
    left: 0 !important;
  }
  .nav-xs .navss > ul > li > a .badge {
    position: absolute;
    right: 10px;
    top: 4px;
    z-index: 3;
  }
  .nav-xs .navss > ul > li:hover > a i,
  .nav-xs .navss > ul > li:focus > a i,
  .nav-xs .navss > ul > li:active > a i,
  .nav-xs .navss > ul > li.active > a i {
    margin-top: -50px;
  }
  .nav-xs .navss > ul ul {
    display: none !important;
    position: absolute;
    left: 100%;
    top: 0;
    z-index: 1050;
    width: 220px;
    -webkit-box-shadow: 0 2px 6px rgba(0,0,0,0.1);
    box-shadow: 0 2px 6px rgba(0,0,0,0.1);
  }
  .nav-xs .navss li:hover > ul,
  .nav-xs .navss li:focus > ul,
  .nav-xs .navss li:active > ul {
    display: block !important;
  }
  .nav-xs.nav-xs-right .navss > ul ul {
    left: auto;
    right: 100%;
  }
